@model IEnumerable<BetStatsAssistWeb.Models.FixtureViewModels>

@{
    ViewBag.Title = "Fixtures";
    Layout = "~/Views/Fixtures/About.cshtml";
}


<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>

    <div>

        <ul class="list-group-item navbar-left">
            @foreach (var fixture in Model)
            {
                <li>@Html.ActionLink(fixture.FixDate + ", " + fixture.HomeTeam + " - " + fixture.AwayTeam, "Fixtures", new { id = "3" })</li>
            }
        </ul>


    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")

</body>
</html>
